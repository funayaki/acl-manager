{
  "name": "funayaki/acl-manager",
  "type": "cakephp-plugin",
  "description": "This CakePHP plugin is an interface to manage an ACL protected web application.",
  "homepage": "http://www.alaxos.ch/blaxos/pages/view/34",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "cakephp/cakephp": "^3.5.0",
    "cakephp/acl": "^0.2.6",
    "oldskool/cakephp-js": "dev-master"
  },
  "require-dev": {
    "cakephp/cakephp-codesniffer": "^3.0",
    "phpunit/phpunit": "^5.7|^6.0"
  },
  "autoload": {
    "psr-4": {
      "AclManager\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AclManager\\Test\\": "tests",
      "App\\": "tests/test_app/src/",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests"
    }
  }
}
